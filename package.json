    "start": "electron .",
    "pkg-linux": "electron-packager . --overwrite --platform=linux  --arch=x64  --ignore='.jscsrc|/*-dev|create_tingo_db.js|create_repo.sh|.gitignore|/.idea|/db_test|/app|/node_modules/(babel*|gulp*|electron*|grunt*)|/.idea/' --icon=fav.png --prune=true --out=app_build",
    "pkg-win": "electron-packager . --overwrite --asar=true --platform=win32  --arch=ia32 --ignore='.jscsrc|/*-dev|create_tingo_db.js|create_repo.sh|.gitignore|/.idea|/db_test|/app|/node_modules/(babel*|gulp*|electron*|grunt*)|/.idea/' --icon=fav.ico --prune=true --out=app_build -version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName='Electron work jornal'",
    "pkg-mac": "electron-packager . --overwrite --platform=darwin --arch=x64  --ignore='.jscsrc|/*-dev|create_tingo_db.js|create_repo.sh|.gitignore|/.idea|/db_test|/app|/node_modules/(babel*|gulp*|electron*|grunt*)|/.idea/' --icon=fav.icns --prune=true --out=app_build"
